            <script id="noanswerListTpl" type="text/template">
              <div class="answer_box _noanswerItem{if viewType != 'onlyTitle' && thumbnail != null} img_box{/if}{if encodedTagList == null} no_tag{/if}{if isFirst} first{/if}{if isLast} last{/if}">
